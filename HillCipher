from ctypes import resize
import numpy as np
p_t='cat'
pt=[]
key= np.matrix[(6,14,9)][(2,5,1)][(8,3,3)]
for i in p_t:
    p_t.append(ord(i)-97)
    num_pt = np.matrix(pt)
    num_pt= resize(3,1)
    c=np.dot(key,pt)
    c= (c%26)-97
    ct=c
    c=np.rarry(c)
    c=np.reshape(-1)
    c=c.tolist()
    l=[]
    for i in c:
        l.append(chr(i))
        print(l)
